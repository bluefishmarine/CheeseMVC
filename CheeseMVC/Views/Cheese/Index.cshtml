@{



}
<h1>My Cheeses</h1>

<p><a asp-controller="Cheese" asp-action="Add">Add Cheese</a></p>

@if (ViewBag.cheeses.Count == 0)
{
    <p>No Cheeses Added!</p>

}


<ul>
    @foreach (KeyValuePair<string,string > cheese in ViewBag.cheeses)
    {
        <li>@cheese.Key:@cheese.Value</li>;
    }

</ul>